ifndef config
  config=debug
endif
export config

PROJECTS := selfdestructing-test

.PHONY: all clean help $(PROJECTS)

all: $(PROJECTS)

selfdestructing-test: 
	@echo "==== Building selfdestructing-test ($(config)) ===="
	@${MAKE} --no-print-directory -C . -f selfdestructing-test.make

clean:
	@${MAKE} --no-print-directory -C . -f selfdestructing-test.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "   debug"
	@echo "   release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   selfdestructing-test"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"
